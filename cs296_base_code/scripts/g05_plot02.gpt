set autoscale
unset log
set xtic auto
set ytic auto
set title "plot02"
set xlabel " Iteration Values "
set ylabel " Averaged values over reruns "
set term png
set output "./plots/g05_plot02.png"
set datafile sep ","

plot './data/lab05_g05_data.csv' u 2:3 smooth unique w point title "average step time", \
	'./data/lab05_g05_data.csv' u 2:4 smooth unique w point title "average collision time", \
	'./data/lab05_g05_data.csv' u 2:5 smooth unique w point title "average velocity time", \
	'./data/lab05_g05_data.csv' u 2:6 smooth unique w point title "average position update time"
