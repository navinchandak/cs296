set autoscale
unset log
unset label
set title "errorbars - plot04"
set xlabel " iteration values "
set ylabel " Step time "
set term png
set output "./plots/g05_plot05.png"
sqr(s)=s*s
set table 'stats.dat'
set datafile sep ","
plot './data/lab05_g05_data.csv' u 2:3 smooth unique 
unset table

set table 'stats2.dat'
set datafile sep ","
plot './data/lab05_g29_data.csv' u 2:(sqr($3)) smooth unique 
unset table

set table 'stats3.dat'
set datafile sep " "
plot '< join stats.dat stats2.dat' u 2:4
unset table

plot 'stats3.dat' u 0:1:(sqrt($2-(sqr($1)))) with yerrorbars title "yerrorbars"
